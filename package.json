{
  "name": "@antd-templater/eslint-config",
  "description": "ESlint Config Plugin of Antd-Templater",
  "homepage": "https://github.com/antd-templater/eslint-config",
  "author": "lin pengteng <china.linpengteng@gmail.com>",
  "license": "MIT",
  "version": "1.0.4",
  "keywords": [
    "eslint-config"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antd-templater/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/antd-templater/eslint-config/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup",
    "eslint": "npx eslint --fix --quiet src/**"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.5.0",
    "@eslint/js": "^9.5.0",
    "@stylistic/eslint-plugin": "^2.2.2",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.8",
    "eslint": "^9.5.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.14.1"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  }
}