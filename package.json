{
  "name": "@antd-templater/eslint-config",
  "description": "ESlint Config Plugin of Antd-Templater",
  "homepage": "https://github.com/antd-templater/eslint-config",
  "author": "lin pengteng <china.linpengteng@gmail.com>",
  "license": "MIT",
  "version": "1.0.0",
  "keywords": [
    "eslint-config"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antd-templater/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/antd-templater/eslint-config/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup",
    "eslint": "npx eslint --fix --quiet src/**",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/config-inspector": "^0.4.8",
    "@eslint/js": "^9.2.0",
    "@stylistic/eslint-plugin": "^1.8.0",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.12.8",
    "cz-message-helper": "^1.3.1",
    "eslint": "^9.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "config": {
    "cz-message-helper": {
      "config": ".cz-message.cjs"
    },
    "commitizen": {
      "path": "node_modules/cz-message-helper"
    }
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "dependencies": {
    "tsup": "^8.0.2"
  }
}